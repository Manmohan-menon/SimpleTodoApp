{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-native-community/masked-view": "~0.1.11",
    "@react-navigation/native": "~6.1.6",
    "@react-navigation/stack": "*",
    "expo": "~47.0.14",
    "expo-constants": "~14.0.2",
    "expo-local-authentication": "~13.0.2",
    "expo-status-bar": "~1.4.2",
    "react": "^18.1.0",
    "react-native": "0.70.8",
    "react-native-gesture-handler": "~2.8.0",
    "react-native-paper": "4.9.2",
    "react-native-reanimated": "~2.12.0",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@testing-library/react-native": "^12.1.2",
    "jest": "^29.5.0",
    "react-test-renderer": "18.1.0"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|react-clone-referenced-element|react-navigation|@react-navigation/.*|@react-native/polyfills))"
    ],
    "moduleNameMapper": {
      "^expo-constants$": "<rootDir>/__mocks__/expo-constants.mock.js",
      "^expo-vector-icons$": "<rootDir>/__mocks__/expo-vector-icons.mock.js"
    },
    "setupFiles": [
      "./jest.setup.js"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    }
  },
  "private": true
}
